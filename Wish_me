#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() 
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>=4 && a<=11){
        printf(" Good Morning");
    }
    if(a>=12 && a<=15 ){
        printf("Good AfterNoon");
    }
    if(a>=16 && a<=20){
        printf("Good Evening");
    }
    if(a>=21&& a<=3 ){
        printf("Good Night");
    }
    return 0;
}
